#: glib/gconvert.c:627 glib/gutf8.c:906 glib/gutf8.c:1106 glib/gutf8.c:1247
#: glib/gutf8.c:1351
msgid "Partial character sequence at end of input"
msgstr "Čiastočná sekvencia znakov na konci vstupu"

#: glib/gconvert.c:800
#, c-format
