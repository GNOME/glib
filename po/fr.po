#: gio/gsettings-tool.c:678
#, c-format
msgid ""
"Unknown command %s\n"
"\n"
msgstr ""
"Commande inconnue %s\n"
"\n"

#: gio/gapplication-tool.c:105
msgid "Usage:\n"
msgstr "Utilisation :\n"

#: gio/gapplication-tool.c:118 gio/gresource-tool.c:559
#: gio/gsettings-tool.c:713
msgid "Arguments:\n"
msgstr "Paramètres :\n"

