#: glib/gregex.c:233
msgid "unrecognized character after (?"
msgstr "neprepoznan znak za (?"

#: glib/gregex.c:237
msgid "unrecognized character after (?<"
msgstr "neprepoznan znak za (?<"

#: glib/gregex.c:241
msgid "unrecognized character after (?P"
msgstr "neprepoznan znak za (?P"

#: glib/gregex.c:244
