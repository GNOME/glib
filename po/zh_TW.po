#: gio/gdbusauthmechanismsha1.c:299
#, c-format
msgid "Error creating directory “%s”: %s"
msgstr "建立目錄「%s」時發生錯誤：%s"

#: gio/gdbusauthmechanismsha1.c:346
#, c-format
msgid "Error opening keyring “%s” for reading: "
msgstr "開啟鑰匙圈「%s」讀取時發生錯誤："

